{
	"configuration": {
		"name": "Default",
		"appenders": {
			"Console": {
				"name": "Console-Appender",
				"target": "SYSTEM_OUT",
				"PatternLayout": {
					"pattern": "%d %p [%c] - %m%n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "error",
				"appender-ref": {
					"ref": "Console-Appender"
				}
			}
		}
	}
}